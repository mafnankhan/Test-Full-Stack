{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "~14.16.1",
    "npm": "~6.14.12"
  },
  "scripts": {
    "start": "npm run watch && npm run dev",
    "dev": "ts-node-dev --transpile-only --no-notify src/index.ts",
    "build": "npm run build-ts && npm run build-js",
    "build-ts": "tsc",
    "build-js": "copyfiles -u 1 'src/**/*.{js,json}' dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,JavaScript,Node,Codegen\" -c \"cyan.bold,blue.bold,green.bold,yellow.bold\" npm:watch-ts npm:watch-js npm:watch-node",
    "watch-ts": "tsc -w",
    "watch-js": "nodemon --watch src --exec 'npm run build-js'",
    "watch-node": "nodemon --delay 1 --watch dist --watch typeDefs -e 'js,json,graphql' dist",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "4.0.1",
    "body-parser": "^1.16.0",
    "cors": "2.8.1",
    "crypto": "^1.0.1",
    "express": "^4.14.0",
    "express-validator": "^6.12.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "passport": "0.3.2",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "request": "2.79.0",
    "sequelize": "^6.2.4",
    "sequelize-cli": "^6.1.0",
    "throng": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/body-parser": "^1.19.0",
    "@types/copyfiles": "^2.2.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@types/nunjucks": "^3.1.3",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/sharp": "^0.25.1",
    "@types/throng": "^4.0.2",
    "babel-jest": "^26.1.0",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.3.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.2",
    "pretty-quick": "2.0.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
